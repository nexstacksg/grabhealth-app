{
  "kind": "collectionType",
  "collectionName": "audit_logs",
  "info": {
    "singularName": "audit-log",
    "pluralName": "audit-logs",
    "displayName": "Audit Log",
    "description": "System audit logs for tracking user actions"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "action": {
      "type": "string",
      "required": true
    },
    "entity": {
      "type": "string",
      "required": true
    },
    "entityId": {
      "type": "string"
    },
    "changes": {
      "type": "text"
    },
    "ipAddress": {
      "type": "string"
    },
    "userAgent": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}